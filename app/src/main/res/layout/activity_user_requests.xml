<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserRequestsActivity">

    <Button
        android:id="@+id/button6"
        android:layout_width="67dp"
        android:layout_height="46dp"
        android:layout_marginStart="256dp"
        android:layout_marginTop="96dp"
        android:backgroundTint="#2196F3"
        android:text="Reply"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <Button
        android:id="@+id/button5"
        android:layout_width="67dp"
        android:layout_height="43dp"
        android:layout_marginStart="268dp"
        android:layout_marginTop="132dp"
        android:backgroundTint="#2196F3"
        android:text="Reply"
        app:layout_constraintStart_toEndOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="USER REQUESTS"
        android:textColor="#090909"
        android:textSize="24sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="50dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="67dp"
        android:layout_height="43dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="72dp"
        android:backgroundTint="#2196F3"
        android:text="Reply"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="72dp"
        android:text="COM BANK"
        android:textColor="#0E0E0E"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="57dp"
        android:layout_height="44dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        android:text="IFS"
        android:textAlignment="textStart"
        android:textColor="#090909"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Virtuza"
        android:textAlignment="textStart"
        android:textColor="#090909"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textView21"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="204dp"
        android:layout_height="76dp"
        android:layout_marginTop="8dp"
        android:text="You have satisfied the prerequisites for the Job Request application. For more information, see Job Requests"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        tools:layout_editor_absoluteX="8dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="196dp"
        android:layout_height="49dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Register today for this segment of an upcoming Amazon Web Services (AWS) webinar series, featuring Eric Martinez, CEO, and Founder of modjoul. Learn how he partnered with Virtusa and AWS to create a mobile app and intelligent workplace  "
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="206dp"
        android:layout_height="72dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:text="Financial institutions are experiencing a wave of transformation as emerging technology and market conditions impact client behavior. To help banks retain clients and remain relevant in a competitive industry, we designed Virtu"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

</androidx.constraintlayout.widget.ConstraintLayout>